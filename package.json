{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --cache --fix --ext .js",
    "format": "prettier --loglevel warn --write \"src/**/*.{ts,tsx}\"",
    "serve": "firebase emulators:start --import=./firebase.data --export-on-exit",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "pretest": "yarn run lint",
    "posttest": "yarn run format",
    "build": "tsc"
  },
  "dependencies": {
    "effector": "^22.3.0",
    "effector-react": "^22.0.6",
    "expo": "^45.0.5",
    "expo-firebase-recaptcha": "~2.2.0",
    "expo-status-bar": "~1.3.0",
    "firebase": "^9.8.3",
    "isomorphic-webcrypto": "^2.3.8",
    "phone": "^3.1.20",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "^0.68.2",
    "react-native-dotenv": "^3.3.1",
    "react-native-web": "0.17.7",
    "react-native-webview": "11.18.1",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": ">=2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "license": "MIT",
  "private": true
}
